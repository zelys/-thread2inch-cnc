;PARAMETROS
R1=0
R2=R1
R3=0.5        ;INCREMENTO POR CICLO
R4=300        ;LARGO DE LA ROSCA
R5=50.8       ;PASO DE LA ROSCA
R6=28         ;DISTANCIA ENTRE FILETES
R7=26         ;PROFUNDIDAD DEL FILETE
R8=0.3        ;INCREMENTO EN X
R9=6          ;INCREMENTO EN Z
R10=R3        ;INCREMENTO DESPUÃ‰S DEL CICLO
;------------------------------------------
;PROGRMAMA
G18 G90 G71 G97 G54
T1D1
S18 M4
DIAMOF
;PRIMER CICLO
CICLO:
;CONDICION 1
IF R1+R8 < R3
R1=R1+R8
ELSE
R1=R3
ENDIF
G0 X0
G0 Z0
G0 X=-R1 Z=-R1
G33 Z=-R4 K=R5
G0 X20
G0 Z0
IF R1 < R3 GOTO CICLO
;SEGUNDO CICLO
R2=R1
CICLO2:
;CONDICION 2
IF R2+R9 < R6
R2=R2+R9
ELSE
R2=R6
ENDIF
G0 X=-R1
G0 Z=-R2
G33 Z=-R4 K=R5
G0 X20
G0 Z0
IF R2 < R6 GOTO CICLO2
;CONDICION FINAL
IF R3 < R7
R3=R3+R10
GOTO CICLO
ELSE
IF R3 == R7
M30
ELSE
R3=R7
GOTO CICLO
ENDIF